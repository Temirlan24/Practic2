using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
           
            double x;
            double.TryParse(Console.ReadLine(), out x);
            double y = 7 * Math.Pow(x,2) - 3 * x + 4;
            Console.WriteLine(y);
            Console.ReadKey();
        }

    }
}
